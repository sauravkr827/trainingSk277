<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="de.hybris.platform.commercefacades.product.data.ProductData">
	<property name="genders" type="java.util.List&lt;de.hybris.trainingSk277.facades.product.data.GenderData>"/>
		<property name="manufacturerDetails" type="java.util.Collection&lt;de.hybris.trainingSk277.facades.product.data.ManufacturerDetailsData>"/>
      <property name="isNewProduct" type="Boolean"/>
	</bean>


	<bean class="de.hybris.trainingSk277.facades.product.data.ManufacturerDetailsData">
		<property name="id" type="Integer"/>
		<property name="name" type="String"/>
		<property name="city" type="String"/>
		<property name="country" type="de.hybris.platform.commercefacades.user.data.CountryData"/>
	</bean>

	<bean class="de.hybris.platform.commercefacades.product.data.StudentData">
		<property name="project" type="java.util.List&lt;de.hybris.trainingSk277.facades.product.data.ProjectData>"/>
	</bean>


	<bean class="de.hybris.trainingSk277.facades.product.data.GenderData">
		<property name="code" type="String"/>
		<property name="name" type="String"/>
	</bean>


	<bean class="de.hybris.trainingSk277.facades.product.data.ProjectData">
		<property name="pId" type="Integer"/>
		<property name="pName" type="String"/>
	</bean>

	<enum class="de.hybris.platform.commercefacades.product.ProductOption">
		<value>ManufacturerDetails</value>
	</enum>

</beans>
